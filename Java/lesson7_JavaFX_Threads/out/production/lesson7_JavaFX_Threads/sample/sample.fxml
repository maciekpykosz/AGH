<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="602.0" prefWidth="775.0">
         <children>
            <ProgressBar fx:id="progressBar" layoutX="124.0" layoutY="549.0" prefHeight="27.0" prefWidth="613.0" progress="0.0" />
            <TextField fx:id="textField" alignment="CENTER" layoutX="214.0" layoutY="437.0" prefHeight="38.0" prefWidth="118.0" />
            <Label layoutX="55.0" layoutY="544.0" prefHeight="38.0" prefWidth="70.0" text="Progress:">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Button layoutX="388.0" layoutY="437.0" mnemonicParsing="false" onAction="#handleRunButtonAction" prefHeight="96.0" prefWidth="159.0" text="START">
               <font>
                  <Font size="20.0" />
               </font></Button>
            <Canvas fx:id="canvas" height="353.0" layoutX="30.0" layoutY="31.0" width="707.0" />
            <Button layoutX="581.0" layoutY="437.0" mnemonicParsing="false" onAction="#handleStopButtonAction" prefHeight="96.0" prefWidth="147.0" text="STOP">
               <font>
                  <Font size="20.0" />
               </font></Button>
            <Label layoutX="55.0" layoutY="437.0" prefHeight="38.0" prefWidth="159.0" text="Enter amount of points:">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Label layoutX="55.0" layoutY="494.0" prefHeight="38.0" prefWidth="46.0" text="Result:">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Label fx:id="label" layoutX="111.0" layoutY="494.0" prefHeight="38.0" prefWidth="206.0" text="here will be result">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </AnchorPane>
   </children>
</GridPane>
